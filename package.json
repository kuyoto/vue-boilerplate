{
  "name": "vue-boilerplate",
  "version": "0.0.0",
  "private": true,
  "description": "A boilerplate for medium to large scale Vue single page applications.",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "lint:eslint": "elint --fix",
    "lint:prettier": "prettier --write --log-level warn",
    "lint:fix": "vue-cli-service lint --fix",
    "test:unit": "vue-cli-service test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuyoto/vue-boilerplate.git"
  },
  "keywords": [
    "vue",
    "boilerplate",
    "spa",
    "application",
    "webpack",
    "hot",
    "reload",
    "router",
    "vuetify",
    "material",
    "vuex"
  ],
  "author": "Tolulope Kuyoro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuyoto/vue-boilerplate/issues"
  },
  "homepage": "https://github.com/kuyoto/vue-boilerplate#readme",
  "dependencies": {
    "axios": "0.19.2",
    "core-js": "3.6.5",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vuetify": "2.3.4",
    "vue-meta": "2.4.0",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-eslint": "4.4.6",
    "@vue/cli-plugin-unit-jest": "4.4.6",
    "@vue/cli-service": "4.4.6",
    "@vue/cli-test-utils": "^4.4.6",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-standard": "5.1.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-imports": "^1.5.1",
    "eslint": "^7.4.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.2.2",
    "eslint-plugin-vuetify": "1.0.0-beta.7",
    "sass": "1.26.10",
    "sass-loader": "9.0.2",
    "vue-cli-plugin-vuetify": "2.0.6",
    "vuetify-loader": "1.6.0",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.11"
  }
}
